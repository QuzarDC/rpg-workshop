{
  "name": "dnd-stat-block-creator",
  "version": "1.0.0",
  "homepage": "https://dnd-stat-block-creator.firebaseapp.com/",
  "description": "A custom stat-block generator for Dungeons & Dragons",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "scss": "node-sass src/_assets/scss/main.scss src/_assets/css/main.css",
    "scss:watch": "npm run scss && node-sass --watch src/_assets/scss/main.scss --output-style compressed --source-map true --output src/_assets/css",
    "cover": "npm test -- --coverage"
  },
  "author": "Joe Ziemba",
  "license": "ISC",
  "dependencies": {
    "firebase": "^5.11.1",
    "gh-pages": "^2.1.1",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-bootstrap-typeahead": "^3.4.7",
    "react-dom": "^16.12.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-toastify": "^5.4.1",
    "reactstrap": "^7.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "node-sass": "^4.13.0"
  }
}
